# Commonly mis-heard code names...
colin new line => 'colon','new','line'
colan new line => 'colon','new','line'
harow => 'arrow'
hero => 'arrow'
herro => 'arrow'
jason => 'json'
name jason => "jason"
h t m l => 'html'
h te m l => 'html'
h t hm l => 'html'
cutie => 'Qt'

dunder in it => 'dunder','init'
thunder in it => 'dunder','init'
thunder main => 'dunder','main'

get diff => 'git','diff'
get push => 'git','push'

not equal => '!='
double equal => '=='
double equals => '=='
asterisk asterisk  => '**'^
star star => ^'**'^
arrow symbol => '=>'
divided by => "/"
times equal  => "*="
